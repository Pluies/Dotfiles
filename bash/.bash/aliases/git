alias 10lc='git lg -10'
alias gf='git fetch --all --prune'
alias gp='git pull --ff-only'
alias brn='git rev-parse --abbrev-ref HEAD'
alias gitback='gf && git checkout main && git pull --ff-only'
alias cleanupbranches='for branch_to_cleanup in $(git branch | grep -v main); do git branch -D $branch_to_cleanup ; done'
